{
  "parser": "babel-eslint",
  "plugins": ["react", "react-hooks"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "document": true,
    "window": true,
    "console": true
  },
  "rules": {
    "no-template-curly-in-string": 0,
    "react/prop-types": "off",
    "no-unused-vars": "warn"
  }
}
